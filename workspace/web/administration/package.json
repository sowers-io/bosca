    "typescript": "^5.7.2",
    "unocss": "^0.65.2",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-shadcn": "^0.3.1",
    "vaul-vue": "^0.2.0",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
