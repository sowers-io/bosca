    "@tanstack/vue-table": "^8.20.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "@tiptap/core": "^2.11.5",
    "@tiptap/suggestion": "^2.11.5",
