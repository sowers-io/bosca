    "@nuxt/icon": "^1.10.3",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/nuxt": "^0.65.2",
    "@vee-validate/zod": "^4.15.0",
