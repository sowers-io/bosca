{
  "name": "bosca-administration",
  "type": "module",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "scripts": {
    "nuxt-build": "nuxt build",
    "dev": "nuxt dev",
    "lint": "deno lint",
    "format": "deno fmt --unstable-component",
    "deno-dev": "deno task dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "vue-tsc --noEmit",
    "codegen": "graphql-codegen && deno run install-queries",
    "install-queries": "cd installer && deno run -A main.ts",
    "build": "deno task nuxt-build",
    "compile": "cp -r public .output/server && cp deno.json .output/server && cd .output/server && deno install --allow-scripts=npm:maplibre-gl@2.4.0,npm:vue-demi@0.14.10,npm:json-editor-vue@0.17.3 && deno compile -A --output ../bosca-administration --include ../public ./index.mjs",
    "compile-and-run": "deno task compile && ./.output/bosca-administration"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@internationalized/date": "^3.6.0",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/extension-superscript": "^2.11.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "@tiptap/core": "^2.11.5",
    "@tiptap/suggestion": "^2.11.5",
    "@tiptap/extension-image": "^2.11.5",
    "@tiptap/extension-document": "^2.11.5",
    "@tiptap/extension-placeholder": "^2.11.5",
    "@tiptap/extension-history": "^2.11.5",
    "@tiptap/extension-underline": "^2.11.5",
    "@tiptap/extension-text-align": "^2.11.5",
    "@unovis/ts": "^1.5.1",
    "@unovis/vue": "^1.5.1",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^13.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.5.2",
    "json-editor-vue": "^0.17.3",
    "lucide-vue-next": "^0.479.0",
    "media-icons": "^1.1.5",
    "reka-ui": "^2.0.2",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tippy.js": "^6.3.7",
    "vaul-vue": "^0.4.0",
    "vee-validate": "^4.15.0",
    "vidstack": "^1.12.13",
    "vue-sonner": "^1.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@iconify-json/lucide": "^1.2.25",
    "@graphql-codegen/client-preset": "^4.6.1",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/add": "^5.0.3",
    "@nuxt/icon": "^1.10.3",
    "@nuxtjs/color-mode": "^3.5.2",
    "@vueuse/math": "^12.2.0",
    "@vueuse/nuxt": "^12.2.0",
    "date-fns": "^4.1.0",
    "nuxt": "^3.15.4",
    "shadcn-nuxt": "^1.0.3",
    "typescript": "^5.7.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.1.10"
  }
}
