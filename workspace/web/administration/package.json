{
  "name": "bosca-administration",
  "type": "module",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "scripts": {
    "nuxt-build": "nuxt build",
    "dev": "nuxt dev",
    "lint": "deno lint",
    "format": "deno fmt --unstable-component",
    "deno-dev": "deno task dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "vue-tsc --noEmit",
    "codegen": "graphql-codegen",
    "build": "deno task nuxt-build",
    "compile": "cp -r public .output/server && cp deno.json .output/server && cd .output/server && deno install --allow-scripts=npm:maplibre-gl@2.4.0,npm:vue-demi@0.14.10 && deno compile -A --output ../bosca-administration --include public ./index.mjs",
    "compile-and-run": "deno task compile && ./.output/bosca-administration",
    "install-queries": "deno run -A ./install-queries.ts"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-typed-document-node/core": "^3.2.0",
